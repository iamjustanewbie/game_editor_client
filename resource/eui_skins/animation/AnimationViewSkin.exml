    </e:List>
    <e:List id="list_dir" itemRendererSkinName="skins.ItemRendererSkin" right="0" top="130">
        <e:ArrayCollection>
            <e:Array>
                <e:Object a="null" />
                <e:Object a="null" />
                <e:Object a="null" />
