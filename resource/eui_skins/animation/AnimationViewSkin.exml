                <e:Label id="labelDisplay" horizontalCenter="0" verticalCenter="0" size="30" />
            </e:Skin>
        </e:skinName>
    </e:Button>
    <e:List id="list_act" itemRendererSkinName="skins.ItemRendererSkin" left="0" top="130" maxHeight="620" bottom="160">
